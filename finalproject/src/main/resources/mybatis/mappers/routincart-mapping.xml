<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.health.dao.PtcartDAO" >

<insert id="insertptcart" parameterType="int">
 insert into ptcart_table(pu_num,et_num) 
 values(#{pu_num},#{et_num})
</insert>
<select id="selectptcart" resultType="ptcartdto">
select * from  ptcart_table where pu_num=#{pu_num}
</select>

<select id="selectptcartcheck" resultType="int">
select  count(*) from  ptcart_table
where pu_num=#{pu_num} and et_num=#{et_num}
</select>

<delete id="deleteptcart" parameterType="int">
delete from ptcart_table where pu_num = #{pu_num}
</delete>

<delete id="deleteptcartone" parameterType="int">
delete from ptcart_table where pu_num = #{pu_num} and et_num= #{et_num}
</delete>



</mapper>














